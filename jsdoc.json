{
    "tags": {
        "allowUnknownTags": false
    },
    "source": {
        "include": ["src/js", "./README.md"],
        "includePattern": ".js$",
        "excludePattern": "(node_modules/|docs|lib)"
    },
    "plugins": [
        "plugins/markdown"
    ],
    "opts": {
        "template": "node_modules/minami",
        "encoding": "utf8",
        "destination": "../tim-docs/",
        "recurse": true,
        "verbose": true
    },
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false,
        "default": {
            "outputSourceFiles": false
        }
    }
}